<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.industry.mapper.EnterpriseVisitMapper">

    <resultMap id="listEnterpriseVisitsMap" type="EnterpriseVisitDO">
        <result column="is_need_visit_again" property="needVisitAgain"/>
    </resultMap>

    <select id="listEnterpriseVisits" resultMap="listEnterpriseVisitsMap" resultType="EnterpriseVisitDO">
        select id,
               enterprise_id,
               visit_content,
               is_need_visit_again,
               visit_again_time
        from construction_industry_api.t_enterprise_visit
        where enterprise_id = #{enterpriseId}
        order by id desc
    </select>
</mapper>
